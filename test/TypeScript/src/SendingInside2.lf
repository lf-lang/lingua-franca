target TypeScript

reactor Printer {
  input x: number

  reaction(x) {=
    console.log("Inside reactor received:" + x );
    if (x != 1) {
      util.requestErrorStop("ERROR: Expected 1.");
    }
  =}
}

main reactor SendingInside2 {
  timer t
  p = new Printer()

  reaction(t) -> p.x {= p.x = 1; =}
}
