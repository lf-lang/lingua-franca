// This test asserts the relative execution order of startup reactions within a composite reactor.
target Rust

main reactor CompositionInitializationOrder {
  c1 = new Component1()
  c2 = new Component2()

  reaction(startup) {= println!("parent woke up"); =}
}

reactor Component2 {
  reaction(startup) {= println!("c2 woke up"); =}
}

reactor Component1 {
  reaction(startup) {= println!("c1 woke up"); =}
}
