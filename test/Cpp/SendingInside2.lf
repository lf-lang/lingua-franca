target Cpp;
reactor Printer {
    input x:int;
    reaction(x) {=
        std::cout << "Inside reactor received: " << *x.get() << std::endl;
        if (*x.get() != 1) {
            std::cerr << "ERROR: Expected 1." << std::endl;
            exit(1);
        }
    =}
}
main reactor SendingInside2 {
    timer t;
    p = new Printer();
    reaction(t) -> p.x {=
        p.x.set(1);
    =}
}
