// Setup for WCET analysis of Worker
target CCpp;
reactor Source {
    output out1: int;
    output out2: int;
    timer t;
    reaction(t) -> out1, out2 {=
        SET(out1, 5);
        SET(out2, 10);
    =}
}
reactor Work {
    input in1: int;
    input in2: int;
    output out:int;
    reaction(in1, in2) -> out {=
    	int ret;
    	if (in1->value > 10) {
    		ret = in2->value * in1->value;
    	} else {
    		ret = in2->value + in1->value;
    	}
        SET(out, ret);
    =}
}
reactor Print {
    input in:int;
    reaction(in) {=
        printf("Received: %d\n", in->value);
    =}
}

main reactor Wcet {
    source = new Source();
    work = new Work();
    print = new Print();
    
    source.out1 -> work.in1;
    source.out2 -> work.in2;
    work.out -> print.in;
}