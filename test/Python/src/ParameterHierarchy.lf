target Python;

reactor Deep(p0=0) {
  reaction(startup) {=
    if self.p0 != 42:
      print(f"Parameter value is {self.p0}. Should have been 42.")
      exit(1)
    else:
      print("Success.")
  =}
}

reactor Intermediate(p1=10) {
  a0 = new Deep(p0=p1)
}

reactor Another(p2=20) {
  a1 = new Intermediate(p1=p2)
}

main reactor ParameterHierarchy {
  a2 = new Another(p2=42)
}