target Python

reactor TestDouble(expected(1.0, 1.0, 1.0, 1.0)) {
  input t_in
  state count = 0

  reaction(t_in) {=
    print("Received: ", t_in.value)
    if t_in.value != self.expected[self.count]:
      sys.stderr.write("ERROR: Expected {:f}.\n".format(self.expected[self.count]))
      exit(1)
    self.count += 1
  =}
}
