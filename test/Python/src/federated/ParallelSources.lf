/** Test parallel connections for federated execution. */
target Python {
  timeout: 2 sec
}

import Count from "../lib/Count.lf"
import TestCount from "../lib/TestCount.lf"

reactor Destination {
  input[2] in_

  t1 = new TestCount(num_inputs=3)
  t2 = new TestCount(num_inputs=3)

  in_ -> t1.in_, t2.in_
}

federated reactor {
  c1 = new Count()
  c2 = new Count()
  d = new Destination()

  c1.out, c2.out -> d.in_
}
