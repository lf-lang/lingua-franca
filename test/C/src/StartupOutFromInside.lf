target C

reactor Bar {
  output out: int

  reaction(startup) -> out {=
    lf_set(out, 42);
  =}
}

main reactor StartupOutFromInside {
  bar = new Bar()

  reaction(startup) bar.out {=
    printf("Output from bar: %d\n", bar.out->value);
    if (bar.out->value != 42) {
      fprintf(stderr, "Expected 42!\n");
      exit(1);
    }
  =}
}
