target C {
  cmake-include: ["../../c/sendreceive.cmake"],
  files: ["../../c"]
}

reactor Print {
  output out: int

  reaction(startup) -> out named sender
}

// expected parameter is for testing.
reactor Check(expected: int = 42) {
  input in: int

  reaction(in) named receiver
}

main reactor {
  s = new Print()
  p = new Check()
  s.out -> p.in
}
