/**
 * Test for lf_request_stop() in federated execution with decentralized coordination at tag (0,0).
 *
 * @author Soroush Bateni
 */
target C

import Sender, Receiver from "DistributedStopZero.lf"

federated reactor {
  sender = new Sender()
  receiver = new Receiver()

  sender.out -> receiver.in
}
