target C {
  timeout: 0 sec
}

import Count from "../lib/Count.lf"
import TestCount from "../lib/TestCount.lf"

reactor Through {
  input in: int
  output out: int

  reaction(in) -> out {=
    lf_set(out, in->value);
  =}
}

reactor Bank {
  input in: int
  output out: int
  t = new Through()
  in, t.out -> t.in, out
}

main reactor {
  b = new[2] Bank()
  s = new Count()
  t = new TestCount()
  s.out, b.out -> b.in, t.in
}
