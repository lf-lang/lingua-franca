@inproceedings{Whitham:12:Carousel,
   author = {Whitham, Jack and Audsley, Neil},
   title = {Explicit Reservation of Local Memory in a Predictable, Preemptive Multitasking Real-time System},
   booktitle = {Real-Time and Embedded Technology and Applications Symposium (RTAS)},
   note = {The idea is with last-in first-out (LIFO)
schedulers (like EDF), the preempting task is responsible for saving
and restoring anything it displaces in the scratchpad memory (much
like function calls and callee-saved registers). This allows a better
WCET because the preempting task knows how much space it needs, and
the preempted task will have its state restored so WCET doesn't
include how many times it is preempted.},
   abstract = {This paper proposes Carousel, a mechanism to
manage local memory space, i.e. cache or scratchpad memory
(SPM), such that inter-task interference is completely eliminated.
The cost of saving and restoring the local memory state across
context switches is explicitly handled by the preempting task,
rather than being imposed implicitly on preempted tasks. Unlike
earlier attempts to eliminate inter-task interference, Carousel
allows each task to use as much local memory space as it requires,
permitting the approach to scale to large numbers of tasks.
Carousel is experimentally evaluated using a simulator. We
demonstrate that preemption has no effect on task execution
times, and that the Carousel technique compares well to the
conventional approach to handling interference, where worst-case
interference costs are simply added to the worst-case execution
times (WCETs) of lower-priority tasks.},
   url = {http://rtslab.wikispaces.com/file/view/carousel.pdf},
   year = {2012},
   type = {Conference Proceedings}
}

