@inproceedings{Kahn:74:PN,
   author = {Kahn, Gilles},
   title = {The Semantics of a Simple Language for Parallel Programming},
   booktitle = {Proc. of the IFIP Congress 74},
   publisher = {North-Holland Publishing Co.},
   pages = {471--475},
   note = {Discusses some of the semantics of the model developed into a language in Kahn/MacQueen. Sequential processes communicate through channels using only non-blocking writes and blocking reads. Kahn argues that if the actors (processes) are ?continuous? (continuity is over a set of ordered lists of data tokens with a partial order (prefix order) relationship between them), then they are also monotone, and any composition of such processes, including recursive combinations and feedback loops, is determinate. Monotone means that if input X1 produces output Y1 and input X2 produces output Y2, then if X1 is a prefix of X2, then Y1 will be a prefix of Y2. It is a form of causality, but without invoking time.
A combination of such processes is described as a set of equations over sequences, and a solution to the equations (a fixed point) gives a possible trace for the processes. Kahn argues (but does not prove) that continuity implies that there will be exactly one ?minimal? solution (where minimal is in the sense of prefix ordering). The minimal solution results from null sequences at the inputs. Other solutions can then be found by iterative compuation, which works because of the monotone condition.
The overall objective appears to be to be able to prove that a given program has a given behavior. A very simple example of such a proof is given.},
   year = {1974},
   type = {Conference Proceedings}
}

@inproceedings{Kahn:77:PN,
   author = {Kahn, Gilles and MacQueen, D. B.},
   title = {Coroutines and Networks of Parallel Processes},
   booktitle = {Information Processing},
   editor = {Gilchrist, B.},
   publisher = {North-Holland Publishing Co.},
   pages = {993-998},
   note = {This paper talks about a model of computation in which ?channels? enable communication between ?processes?. Inputs and outputs to a process are distinguished from parameters, which have a more functional style. When a process accesses data from a port, if no data is available, the process suspends. A demand-driven style of computation is proposed in which a single ?driver? process (one with no output ports) demands inputs. When it suspends due to an input being unavailable, the channel is marked ?hungry? and the source process is activated. It may in turn suspend, if its inputs are not available. Any process that issues a PUT command to a hungry channel will be suspended and the destination process restarted where it left off. If a GET operation suspends a process, and the source process is already suspended waiting for an input, then deadlock has been detected. It is an important part of the semantics of the language that the availability of inputs cannot be tested. The network may be specified either by giving channels names and binding them to inputs and outputs of processes, or by using a functional notation for processes with only a single output.
Reconfiguration of the network on the fly is allowed. This allows for recursive definition of processes. Recursive defnition of streams (data) is also permitted in the form of directed loops in the process graph.

A program is said to be determinate if the history of every communication variable (stream) is determined by the value of the program?s parameters. Determinacy is guaranteed as long as each channel is bound to a single output port of a single producer.
Imperative ordering of operations within a process definition is allowed, so the language mixes functional, imperative, and dataflow semantics all in one.

Two very interesting examples are given. The first recursively specifies an algorithm for finding prime numbers using the sieve of Eratosthenes. The second uses data recurrences and an ordered merge to compute an ordered sequence of integers of the form  for all . This latter problem had been considered by Dijkstra.},
   year = {1977},
   type = {Conference Proceedings}
}

