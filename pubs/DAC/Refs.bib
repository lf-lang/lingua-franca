@article{Naundorf:00:Causal,
   author = {Naundorf, Holger},
   title = {Strictly causal functions have a unique fixed point},
   journal = {Theoretical Computer Science},
   volume = {238},
   number = {1-2},
   pages = {483-488},
   note = {Defines strictly causal to be where the common prefix of the output is a strict superset of the common prefix of the input
and then proves that a unique fixed point exists. This allows superdense time (cites Perry, VHDL) and partially ordered
tag sets. The solution is not constructive (it uses Zorn's lemma, which requires the axiom of choice). The key theorem also shows that the common prefix of any signal with the unique
fixed point is the same as the common prefix of that any signal s with F(s), where F is the system function.

Also gives an alternative formulation where signals are defined on downsets and shows that for total signals,
the definition is the same. In this latter case, he just needs monotonicity and Knaster-Tarski to conclude that there is a fixed
point, but there may be more than one fixed point, and the fixed point is not necessarily total. },
   url = {http://www.sciencedirect.com/science/article/B6V1G-43FWY1C-K/2/d64dbca6a23fee5596d3b12189ccf34d },
   year = {2000},
   type = {Journal Article}
