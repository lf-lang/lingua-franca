@inproceedings{ReimannEtAl:10:SMT,
   author = {Reimann, Felix and Gla\ss{}, Michael and Haubelt, Christian and Eberl, Michael and Teich, J\"urgen},
   title = {Improving Platform-Based System Synthesis by Satisfiability Modulo Theories Solving},
   booktitle = {CODES+ISSS},
   publisher = {ACM},
   pages = {135-144},
   note = {I just heard a talk at CODES/ISSS that I think provides what
we need to definitely do schedulability analysis on PTIDES
models.  It was quite a beautiful talk. The approach
was to encode scheduling decisions as boolean formulas
and use a SAT solver to find feasible solutions based
on constraints (such as precedences and contention).
Given a valid schedule, the technique can then check
it against "non-functional" constraints (such as deadlines
at actuators) and reject solutions that don't meet
these constraints. It then uses a learning strategy,
modifying the boolean formulas to exclude the
infeasible solution.

But then this gets really pretty:
Instead of running the SAT solver to completion each
time, the "non-functional" constraints are encoded
as a background theory, and an SMT solver is used
instead. Infeasible solutions are identified earlier
in the process. Interestingly, something like a
deadline at an actuator will not normally be a
monotonic background theory, but in this talk,
the speaker explained how to make a monotonic
version by constraining when to apply the background
theory. In the case of checking a deadline, you wouldn't
check until you had scheduled all the actors upstream
from an actuator. But you don't have to have scheduled
the whole graph.},
   year = {2010},
   type = {Conference Proceedings}
}

