// This tests that an output that is not connected to anything does not result
// in a compilation error.  Passing the test is just compiling and running.
target Cpp;
reactor Source {
    output out:int;
    timer t;
    reaction(t) -> out {=
        out.set(1);
    =}
}
reactor Gain {
    preamble {=
        #include <iostream>
    =}
    input in:int;
    output out:int;
    reaction(in) -> out {=
        std::cout << "Received " << *in.get() << std::endl;
        out.set(*in.get() * 2);
    =}
}
main reactor DanglingOutput {
    source = new Source();
    container = new Gain();
    source.out -> container.in;
}
