/**
 * Very basic chain with just two reactors, one that sends
 * a message a startup feeding one that reports receiving it.
 * 
 * @author Edward A. Lee
 */
target C;

import SendOnce, Receive from "lib/SendersAndReceivers.lf";

main reactor {
    r1 = new SendOnce();
    r2 = new Receive();
    r1.out -> r2.in;
}
